    "lint-staged": "^15.2.10",
    "msw": "^2.4.9",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^2.1.1"
